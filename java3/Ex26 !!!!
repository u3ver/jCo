import java.util.Scanner;

class Test {
  public static void main (String[] args) {
    System.out.println ("enter the time in seconds: ");
    Scanner sc = new Scanner(System.in);
      int N = sc.nextInt();
    sc.close();
    
      int min = N / 60;
      int hour = N / 3600;
      int sec = N / min;
    
      System.out.println ( hour + "h. " + min + "m. " + sec + "sec.");
  }
}
